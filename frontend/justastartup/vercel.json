{
  "version": 2,
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_PROJECT_URL": "@vite_project_url",
    "VITE_API_KEY": "@vite_api_key"
  },
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}